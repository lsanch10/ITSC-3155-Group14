<h1>New Client</h1>
<%= form_for :client, url: clients_path do |f| %>
  <p>
    <%= f.label :Name %><br>
    <%= f.text_field :Name %>
  </p>
 
 <p>
    <%= f.label :Address %><br>
    <%= f.text_field :address %>
  </p>
  <p>
    <%= f.label :Phone_Number %><br>
    <%= f.text_field :Phone_Number %>
  </p>
  <p>
    <%= f.label :Email %><br>
    <%= f.text_field :Email %>
  </p>
  <p>
     <p>
    <%= f.label :Court_Date %><br>
    <%= f.text_field :Court_Date %>
  </p>
  <p> 
    <%= f.label :Case_Description %><br>
    <%= f.text_area :Case_Description %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>
  <%= link_to 'Cancel', clients_path %>
